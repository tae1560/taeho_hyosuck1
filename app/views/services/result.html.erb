<div class="container">
<div style="margin-top: 10px;">
  <% if @result %>
  <table>
    <tr>
      <td rowspan="2"><img src="/images/description.png" /></td>
      <td class="admin_title" style="padding-top:30px">RESULT</td>
    </tr>

    <tr>
      <td style="max-width: 700px;">
        <table class="admin_content">
          <tr>
            <td colspan="3">
              <%= @processing.updated_at.in_time_zone("Seoul").time.strftime("%d-%m-%Y %p %I:%M") %> | SCAN <%= @processing.id %> | KOREAN 한국어판 | <%= @processing.name %></td>
          </tr>
          <tr>
            <td> <%= image_tag @result.image1.url %> </td>
            <td> <%= image_tag @result.image2.url %> </td>
            <td></td>
          </tr>
          <tr>
            <td style="font-size: 15px;"> CHARACTER & DISPOSITION </td>
            <td style="font-size: 15px;"> ADVANTAGES AND DISADVANTAGES </td>
            <td></td>
            <!--<td style="text-align: right;"> <img src="/images/open_button.png" />  </td>-->
            <!--<td style="text-align: right;"> <img src="/images/open_button.png" />  </td>-->
          </tr>
          <tr>
            <td><textarea name="text1" style="width: 95%; height: 150px; font-size: 12px;"><%= @result.text1 %></textarea></td>
            <td><textarea name="text2" style="width: 95%; height: 150px; font-size: 12px;"><%= @result.text2 %></textarea></td>
            <td style="min-width: 150px;"><a href="scan" onclick="call_event('rescan');" ><img src="/images/rescan_button.png" /> </a>
              <br/><br/>
              <a href="#" ><img src="/images/print_button.png" onclick="call_event('print'); window.print()" /> </a> </td></td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
  <% else %>
  <h1 style="color: white;"> ERROR </h1>
  <ul style="color: white; font-size: 30px; text-align: left; margin-left: 100px;">
    <% @error_messages.each do |error_message| %>
        <li style=" margin: 30px;"><%= error_message %></li>
    <% end %>
  </ul>
      <a onclick="call_event('cancel'); history.go(-1);">
        <img src="/images/back_button.png">
      </a>

  <% end %>
</div>
</div>